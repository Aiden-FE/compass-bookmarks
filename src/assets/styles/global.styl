@import "@compass-aiden/styles/lib/assets/main.css";
@import 'ant-design-vue/dist/antd.variable.min.css';

html,
body
  color var(--cp-font-color, #212121)
  background var(--cp-page-background, #fff)

#app
  overflow auto
  width 100%
  height 100%

.cp-primary-color
  color var(--cp-primary-color) !important

.cp-absolute-center
  position: absolute;
  margin: auto;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
