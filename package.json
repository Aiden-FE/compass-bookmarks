{
  "name": "compass-bookmarks",
  "private": true,
  "version": "1.0.1",
  "description": "指南针书签前端应用",
  "repository": "https://github.com/Aiden-FE/compass-template/tree/template/vue3-ts",
  "author": "Aiden_FE@outlook.com",
  "license": "MIT",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "preview": "vite preview",
    "commit": "git cz",
    "test": "jest --coverage",
    "release": "standard-version --no-verify -a"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "dependencies": {
    "@compass-aiden/styles": "^1.0.1",
    "@compass-aiden/utils": "^1.5.3",
    "@iconify/json": "^2.1.40",
    "@vueuse/core": "^8.2.6",
    "ant-design-vue": "^3.2.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.13",
    "vue": "^3.2.33",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@compass-aiden/eslint-plugin": "^0.0.2",
    "@iconify-json/ant-design": "^1.1.1",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@nabla/vite-plugin-eslint": "^1.4.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^1.8.1",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.13.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.4.12",
    "standard-version": "^9.3.2",
    "stylus": "^0.57.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-mock-server": "^1.0.4",
    "vite-plugin-windicss": "^1.8.4",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.5.1"
  }
